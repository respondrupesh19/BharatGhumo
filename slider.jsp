<html>
    <head>
        <title>Slider</title>
        <link href="css/bootstrap.css" type="text/css" rel="stylesheet" />
    </head>
    <body>
        <div style=" height: 10px;">
            
        </div>
            <div class="container">
                <div id="myslide" class="carousel slide carousel-fade" data-ride="carousel">
                    <ol class="carousel-indicators mt-3">
                        <li class="active" data-target="#myslide" data-slide-to="0"></li>
                         <li data-target="#myslide" data-slide-to="1"></li>
                         <li data-target="#myslide" data-slide-to="2"></li>
                         <li data-target="#myslide" data-slide-to="3"></li>
                         <li data-target="#myslide" data-slide-to="4"></li>
                         <li data-target="#myslide" data-slide-to="5"></li>
                    </ol>
                    <div class="carousel-inner">
                        <div class="carousel-item ">
                            <img src="img/a1.png" style="width: 1400px; height: 500px;">
                        </div>
                        <div class="carousel-item" >
                            <img src="img/a2.png" style="width: 1400px; height: 500px;">
                        </div>
                        <div class="carousel-item">
                            <img src="img/a3.png" style="width: 1400px; height: 500px;">
                        </div>
                        <div class="carousel-item">
                            <img src="img/a5.png" style="width: 1400px; height: 500px;">
                        </div>
                         <div class="carousel-item">
                            <img src="img/1.jpg" style="width: 1400px; height: 500px;">
                        </div>
                         <div class="carousel-item active">
                            <img src="img/Statue-of-Unity.jpg" style="width: 1400px; height: 500px;">
                        </div>
                       
                    </div>
                    <a href="#myslide" class="carousel-control-prev" role="button" data-slide="prev">
                        <span class="carousel-control-prev-icon"></span>
                    </a>
                    <a href="#myslide" class="carousel-control-next" role="button" data-slide="next">
                        <span class="carousel-control-next-icon"></span>
                    </a>
                </div>
            </div>
            
            
            
        
        
        <script src="js/bootstrap.js"></script>
        <script src="js/jquery.js"></script>
    </body>
</html>
